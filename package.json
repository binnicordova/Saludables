{
    "name": "saludables",
    "main": "expo-router/entry",
    "version": "0.0.14",
    "scripts": {
        "start": "expo start",
        "mocks": "node ./scripts/mock-list-details.js",
        "android": "expo run:android --device",
        "android:release": "expo run:android --device --variant=release",
        "ios": "expo run:ios --device",
        "ios:release": "expo run:ios --device --variant=release",
        "ios:device": "expo run:ios --device \"Binni Cordova's iPhone\"",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "npx @biomejs/biome check",
        "format": "npx @biomejs/biome format --write",
        "build:dev": "eas build --platform all --profile development",
        "build:preview": "eas build --platform all --profile preview",
        "build:prod": "eas build --platform all --profile production",
        "build:prod:android": "eas build --platform android --profile production",
        "update:dev": "eas update --branch development --message \"Development update - $(date '+%Y-%m-%d %H:%M')\"",
        "update:preview": "eas update --branch preview --message \"Preview update - $(date '+%Y-%m-%d %H:%M')\"",
        "update:prod": "eas update --branch production --message \"Production update - $(date '+%Y-%m-%d %H:%M')\"",
        "update:prod:android": "eas update --branch production --message \"Production update - $(date '+%Y-%m-%d %H:%M')\" --platform android",
        "eas-preview": "eas build --platform android --profile preview",
        "types": "tsc --noEmit --skipLibCheck"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/ui": "~0.2.0-beta.7",
        "@expo/vector-icons": "^15.0.3",
        "@react-native-async-storage/async-storage": "2.2.0",
        "@react-native-segmented-control/segmented-control": "2.5.7",
        "@react-navigation/bottom-tabs": "^7.2.0",
        "@react-navigation/native": "^7.0.14",
        "@shopify/flash-list": "2.0.2",
        "expo": "^54.0.21",
        "expo-blur": "~15.0.7",
        "expo-checkbox": "~5.0.7",
        "expo-constants": "~18.0.10",
        "expo-dev-client": "~6.0.16",
        "expo-font": "~14.0.9",
        "expo-haptics": "~15.0.7",
        "expo-image": "~3.0.10",
        "expo-linking": "~8.0.8",
        "expo-localization": "~17.0.7",
        "expo-location": "~19.0.7",
        "expo-router": "~6.0.14",
        "expo-splash-screen": "~31.0.10",
        "expo-status-bar": "~3.0.8",
        "expo-symbols": "~1.0.7",
        "expo-system-ui": "~6.0.8",
        "expo-updates": "~29.0.12",
        "expo-web-browser": "~15.0.8",
        "jotai": "^2.3.0",
        "lottie-react-native": "~7.3.4",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-native": "0.81.5",
        "react-native-executorch": "^0.5.15",
        "react-native-gesture-handler": "~2.28.0",
        "react-native-reanimated": "~4.1.1",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-screens": "~4.16.0",
        "react-native-web": "^0.21.0",
        "react-native-webview": "13.15.0",
        "react-native-worklets": "0.5.1"
    },
    "dependenciesMeta": {
        "jotai": {
            "optional": false
        }
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@biomejs/biome": "1.9.4",
        "@expo/config-types": "^54.0.8",
        "@types/jest": "^29.5.12",
        "@types/react": "~19.1.10",
        "@types/react-test-renderer": "^18.3.0",
        "dotenv": "^17.2.3",
        "jest": "^29.2.1",
        "jest-expo": "~54.0.13",
        "react-test-renderer": "19.1.0",
        "sharp": "^0.34.4",
        "typescript": "^5.3.3"
    },
    "private": true
}